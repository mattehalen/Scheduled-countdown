extends layout

block content
  p(hidden='')
  .topRow()
    div.audiobutton
      button#play.commandButton.btn.btn-secondary(hidden='') Activate Audio
    #nowTopRow.topRowText
  #mainContentBox.row.mainContentBox
  
      
    #centerNowText
      h1#now

    #titleContentBox.titleContentBox
      h1#title
      h1#start
    
  .downSite
    #cueTime.cueTime
    #5minute.fiveMinute
    audio.countDownSound#music0(src='audio/Dummy.mp3')
    audio.countDownSound#music1(src='audio/1min.mp3')
    audio.countDownSound#music2(src='audio/2min.mp3')
    audio.countDownSound#music3(src='audio/3min.mp3')
    audio.countDownSound#music4(src='audio/4min.mp3')
    audio.countDownSound#music5(src='audio/5min.mp3')
    
    audio.countDownSound#musiclong1(src='audio/its-now-1minute.mp3')
    audio.countDownSound#musiclong2(src='audio/its-now-2minutes.mp3')
    audio.countDownSound#musiclong3(src='audio/its-now-3minutes.mp3')
    audio.countDownSound#musiclong4(src='audio/its-now-4minutes.mp3')
    audio.countDownSound#musiclong5(src='audio/its-now-5minutes.mp3')
    audio.countDownSound#musiclong6(src='audio/its-now-6minutes.mp3')
block pagescripts
  // INDEX Scripts
  script(src="/javascripts/script.js")
